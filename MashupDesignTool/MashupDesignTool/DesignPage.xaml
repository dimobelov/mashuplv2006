            <Setter Property="Foreground" Value="#FF4A85CD"></Setter>
            <Setter Property="FontSize" Value="9"></Setter>
            <Setter Property="Template">
                <Setter.Value>
                    <ControlTemplate TargetType="Button">
                        <Grid>
                            <vsm:VisualStateManager.VisualStateGroups>
                                <vsm:VisualStateGroup x:Name="CommonStates">
                                    <vsm:VisualState x:Name="Normal"/>
                                    <vsm:VisualState x:Name="MouseOver">
                                        <Storyboard>
                                            <ColorAnimationUsingKeyFrames BeginTime="00:00:00" 
                                                Duration="00:00:00.0010000" Storyboard.TargetName="GradientStop1" 
                                                Storyboard.TargetProperty="Color">
                                                <SplineColorKeyFrame KeyTime="00:00:00" Value="#FFFBEEA2"/>
                                            </ColorAnimationUsingKeyFrames>
                                            <ColorAnimationUsingKeyFrames BeginTime="00:00:00" 
                                                Duration="00:00:00.0010000" Storyboard.TargetName="GradientStop2" 
                                                Storyboard.TargetProperty="Color">
                                                <SplineColorKeyFrame KeyTime="00:00:00" Value="#FFEFD379"/>
                                            </ColorAnimationUsingKeyFrames>
                                            <ColorAnimationUsingKeyFrames BeginTime="00:00:00" 
                                                Duration="00:00:00.0010000" Storyboard.TargetName="GradientStop3" 
                                                Storyboard.TargetProperty="Color">
                                                <SplineColorKeyFrame KeyTime="00:00:00" Value="#FFE9B91F"/>
                                            </ColorAnimationUsingKeyFrames>
                                            <ColorAnimationUsingKeyFrames BeginTime="00:00:00" 
                                                Duration="00:00:00.0010000" Storyboard.TargetName="GradientStop4" 
                                                Storyboard.TargetProperty="Color">
                                                <SplineColorKeyFrame KeyTime="00:00:00" Value="#FFE1B720"/>
                                            </ColorAnimationUsingKeyFrames>
                                        </Storyboard>
                                    </vsm:VisualState>
                                    <vsm:VisualState x:Name="Pressed">
                                        <Storyboard>
                                            <ColorAnimationUsingKeyFrames BeginTime="00:00:00" 
                                                Duration="00:00:00.0010000" Storyboard.TargetName="GradientStop1" 
                                                Storyboard.TargetProperty="Color">
                                                <SplineColorKeyFrame KeyTime="00:00:00" Value="#FFF1E3D0"/>
                                            </ColorAnimationUsingKeyFrames>
                                            <ColorAnimationUsingKeyFrames BeginTime="00:00:00" 
                                                Duration="00:00:00.0010000" Storyboard.TargetName="GradientStop2" 
                                                Storyboard.TargetProperty="Color">
                                                <SplineColorKeyFrame KeyTime="00:00:00" Value="#FFD3B07C"/>
                                            </ColorAnimationUsingKeyFrames>
                                            <ColorAnimationUsingKeyFrames BeginTime="00:00:00" 
                                                Duration="00:00:00.0010000" Storyboard.TargetName="GradientStop3" 
                                                Storyboard.TargetProperty="Color">
                                                <SplineColorKeyFrame KeyTime="00:00:00" Value="#FFC8780E"/>
                                            </ColorAnimationUsingKeyFrames>
                                            <ColorAnimationUsingKeyFrames BeginTime="00:00:00" 
                                                Duration="00:00:00.0010000" Storyboard.TargetName="GradientStop4" 
                                                Storyboard.TargetProperty="Color">
                                                <SplineColorKeyFrame KeyTime="00:00:00" Value="#FFEA7E15"/>
                                            </ColorAnimationUsingKeyFrames>
                                        </Storyboard>
                                    </vsm:VisualState>
                                    <vsm:VisualState x:Name="Disabled">
                                        <Storyboard/>
                                    </vsm:VisualState>
                                </vsm:VisualStateGroup>
                                <vsm:VisualStateGroup x:Name="FocusStates">
                                    <vsm:VisualState x:Name="Focused">
                                        <Storyboard/>
                                    </vsm:VisualState>
                                    <vsm:VisualState x:Name="Unfocused"/>
                                </vsm:VisualStateGroup>
                            </vsm:VisualStateManager.VisualStateGroups>
                            <Border CornerRadius="7"
                                       BorderBrush="#FF8393A9" BorderThickness="1" 
                                       x:Name="border" RenderTransformOrigin="0.5,0.5">
