            <Popup.Child>
                <Grid Width="400" Height="200">
                    <!--<my1:OuterGlowBorder CornerRadius="10" Margin="10" 
                                         OuterGlowOpacity="0.5" OuterGlowSize="20"
                                         HorizontalAlignment="Center" VerticalAlignment="Center" BorderThickness="0"
                                         Style="{StaticResource PopupBorderStyle}">-->
                    <Border HorizontalAlignment="Center" VerticalAlignment="Center"
                            Style="{StaticResource PopupBorderStyle}" 
                            CornerRadius="5" Margin="10">
                        <Border.Effect>
                            <DropShadowEffect BlurRadius="30" Color="Black" Direction="0" Opacity="1" ShadowDepth="0"></DropShadowEffect>
                        </Border.Effect>
                        <StackPanel x:Name="spPopupLoading" Orientation="Vertical" 
                                    VerticalAlignment="Center" HorizontalAlignment="Center" Style="{StaticResource PopupBackgroundStyle}">
                            <StackPanel.Resources>
                                <Storyboard x:Name="openPopup">
                                    <DoubleAnimation Duration="0:0:.4" From="0" To="300"
                                                 Storyboard.TargetName="spPopupLoading" 
                                                 Storyboard.TargetProperty="Width"></DoubleAnimation>
                                    <DoubleAnimation Duration="0:0:.4" From="0" To="150"
                                                 Storyboard.TargetName="spPopupLoading" 
                                                 Storyboard.TargetProperty="Height"></DoubleAnimation>
                                </Storyboard>
                                <Storyboard x:Name="closePopup">
                                    <DoubleAnimation Duration="0:0:.3" To="0"
                                                 Storyboard.TargetName="spPopupLoading" 
                                                 Storyboard.TargetProperty="Width"></DoubleAnimation>
                                    <DoubleAnimation Duration="0:0:.3" To="0"
                                                 Storyboard.TargetName="spPopupLoading" 
                                                 Storyboard.TargetProperty="Height"></DoubleAnimation>
                                </Storyboard>
                            </StackPanel.Resources>
                            <my1:LoadingAnimation AutoPlay="True" Margin="0,5" Style="{StaticResource PopupLoadingAnimationStyle}">
                                <TextBlock x:Name="popupText" Text="L O A D I N G ..." VerticalAlignment="Center" HorizontalAlignment="Center" FontFamily="Verdana" FontSize="12"></TextBlock>
                            </my1:LoadingAnimation>
                            <Button Content="Cancel" Width="60" Height="20" VerticalAlignment="Center" HorizontalAlignment="Center" Click="Button_Click" Style="{StaticResource PopupButtonCancelStyle}"></Button>
                        </StackPanel>
                        <!--</my1:OuterGlowBorder>-->
                    </Border>
                </Grid>
            </Popup.Child>
        </Popup>
        
        <MapulRibbon:Ribbon HorizontalAlignment="Stretch" Name="menu" 
                            VerticalAlignment="Stretch" Margin="0,0,0,0" 
                            Grid.Row="0" Height="145" 
                            Title="Silverlight Designer Tools"
                            OnSelectionTabChanged="menu_OnSelectionTabChanged">
            <MapulRibbon:Tabs>
                <MapulRibbon:Tabs.Items>
                    <MapulRibbon:TabsItem Header="Home">
                        <MapulRibbon:TabsItem.Content>
                            <MapulRibbon:RibbonItems>
                                <MapulRibbon:RibbonItem Title="File">
                                    <MapulRibbon:RibbonItem.Content>
                                        <MapulRibbon:RibbonButtonsGroup Orientation="Horizontal">
                                            <MapulRibbon:RibbonButton x:Name="btnNew" ImageUrl="Images/new.png" Text="New"
                                                                      TooltipTitle="New" 
                                                                      TooltipText="New" OnClick="btnNew_OnClick">
                                                <!--<MapulRibbon:RibbonButton.RibbonList>
                                                    <MapulRibbon:RibbonList>
                                                        <MapulRibbon:RibbonList.Items>
                                                            <MapulRibbon:RibbonListItem x:Name="btnNewPage" ImageSource="Images/new.png" Text="New page" />
                                                            <MapulRibbon:RibbonListItem x:Name="btnNewSite" ImageSource="Images/new.png" Text="New site" />
                                                        </MapulRibbon:RibbonList.Items>
                                                    </MapulRibbon:RibbonList>
                                                </MapulRibbon:RibbonButton.RibbonList>-->
                                            </MapulRibbon:RibbonButton>
                                            <!--<MapulRibbon:RibbonButtonsGroup Orientation="Horizontal">
                                                <MapulRibbon:RibbonButton x:Name="btnOpen" ImageUrl="Images/open.png" Text="Open"
                                                                            TooltipTitle="Open"
                                                                            TooltipText="Open page or site">
                                                    <MapulRibbon:RibbonButton.RibbonList>
                                                        <MapulRibbon:RibbonList>
                                                            <MapulRibbon:RibbonList.Items>
                                                                <MapulRibbon:RibbonListItem x:Name="btnOpenPage" ImageSource="Images/open.png" Text="Open page" />
                                                                <MapulRibbon:RibbonListItem x:Name="btnOpenSite" ImageSource="Images/open.png" Text="Open site" />
                                                            </MapulRibbon:RibbonList.Items>
                                                        </MapulRibbon:RibbonList>
                                                    </MapulRibbon:RibbonButton.RibbonList>
                                                </MapulRibbon:RibbonButton>
                                            </MapulRibbon:RibbonButtonsGroup>-->
                                            <MapulRibbon:RibbonButtonsGroup>
                                                <MapulRibbon:RibbonButton x:Name="btnSave" ImageUrl="Images/save.png" Text="Save"
                                                                                TooltipTitle="Save"
                                                                                TooltipText="Save" OnClick="btnSave_OnClick">
                                                </MapulRibbon:RibbonButton>
                                            </MapulRibbon:RibbonButtonsGroup>
                                            <MapulRibbon:RibbonButtonsGroup>
                                                <MapulRibbon:RibbonButton x:Name="btnSaveAs" ImageUrl="Images/saveas.png" Text="Save As"
                                                                                TooltipTitle="Save As"
                                                                                TooltipText="Save As" OnClick="btnSaveAs_OnClick">
                                                </MapulRibbon:RibbonButton>
                                            </MapulRibbon:RibbonButtonsGroup>
                                        </MapulRibbon:RibbonButtonsGroup>
                                    </MapulRibbon:RibbonItem.Content>
                                </MapulRibbon:RibbonItem>
                                <MapulRibbon:RibbonItem Title="View">
                                    <MapulRibbon:RibbonItem.Content>
                                        <MapulRibbon:RibbonButtonsGroup Orientation="Horizontal">
                                            <MapulRibbon:RibbonButton Name="btnPreview" ImageUrl="Images/Preview.png" Text="Preview" Height="70"
                                                                      TooltipTitle="Preview" TooltipText="Preview page" OnClick="btnPreview_OnClick">
                                            </MapulRibbon:RibbonButton>
                                            <MapulRibbon:RibbonButton Name="btnFullScreen" ImageUrl="Images/fullscreen.png" Text="Switch" Height="70"
                                                                    TooltipTitle="Switch/Escape full screen mode" TooltipText="Switch to/escape full screen mode" OnClick="btnFullScreen_OnClick">
                                            </MapulRibbon:RibbonButton>
                                        </MapulRibbon:RibbonButtonsGroup>
                                    </MapulRibbon:RibbonItem.Content>
                                </MapulRibbon:RibbonItem>
                            </MapulRibbon:RibbonItems>
                        </MapulRibbon:TabsItem.Content>
                    </MapulRibbon:TabsItem>
                </MapulRibbon:Tabs.Items>
            </MapulRibbon:Tabs>
        </MapulRibbon:Ribbon>
        
        <Grid Height="2" Style="{StaticResource HRLineStyle}" Grid.Row="1">
        </Grid>

        <Grid Grid.Row="2" Style="{StaticResource MainGridStyle}">
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="150" x:Name="ToolbarColumn"></ColumnDefinition>
                <ColumnDefinition Width="3"></ColumnDefinition>
                <ColumnDefinition Width="6*"></ColumnDefinition>
                <ColumnDefinition Width="3"></ColumnDefinition>
                <ColumnDefinition Width="250" x:Name="PropertiesGridColumn"></ColumnDefinition>
            </Grid.ColumnDefinitions>

            <Border Grid.Column="0" x:Name="ToolbarPanel" Style="{StaticResource PanelStyle}">
            <!--<my1:InnerGlowBorder BorderThickness="0" CornerRadius="0,0,10,10" 
                                 InnerGlowOpacity="0.5" InnerGlowSize="15"
                                 HorizontalAlignment="Stretch" VerticalAlignment="Stretch" 
                                 x:Name="ToolbarPanel"
                                 Style="{StaticResource PanelStyle}">-->
                <Grid>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="*"></ColumnDefinition>
                        <ColumnDefinition Width="19"></ColumnDefinition>
                    </Grid.ColumnDefinitions>
                    <Grid.RowDefinitions>
                        <RowDefinition Height="19"></RowDefinition>
                        <RowDefinition Height="*"></RowDefinition>
                    </Grid.RowDefinitions>

                    <ScrollViewer VerticalAlignment="Stretch" HorizontalAlignment="Stretch" Grid.Row="1" Grid.ColumnSpan="2"
                                  HorizontalScrollBarVisibility="Auto" VerticalScrollBarVisibility="Auto" x:Name="treeControlScrollViewer"  
                                  Style="{StaticResource TreeControlScrollViewerStyle}">
                        <ItemsControl x:Name="treeControl">
                            <ItemsControl.ItemTemplate>
                                <DataTemplate>
                                    <my1:AnimatedExpander Header="{Binding GroupName}" FontSize="12" Style="{StaticResource GroupControlHeaderStyle}" FontFamily="Verdana" Margin="0">
                                        <ItemsControl Margin="0" ItemsSource="{Binding ControlInfos}">
                                            <ItemsControl.ItemTemplate>
                                                <DataTemplate>
                                                    <RadioButton Margin="8,0,6,0" Height="Auto" Width="Auto" x:Name="Control" Style="{StaticResource RadioButtonControlStyle}" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Click="Control_Click">
                                                        <StackPanel Orientation="Horizontal">
                                                            <Image Width="15" Height="15" Source="{Binding IconName}" Stretch="Uniform"></Image>
                                                            <TextBlock Margin="2,0,2,0" FontFamily="Verdana" FontSize="11" Text="{Binding DisplayName}" UseLayoutRounding="False" Style="{StaticResource TextBlockControlDisplayNameStyle}" TextTrimming="WordEllipsis" />
                                                        </StackPanel>
                                                        <ToolTipService.ToolTip>
                                                            <ToolTip Content="{Binding Description}"></ToolTip>
                                                        </ToolTipService.ToolTip>
                                                    </RadioButton>
                                                </DataTemplate>
                                            </ItemsControl.ItemTemplate>
                                        </ItemsControl>
                                    </my1:AnimatedExpander>
                                </DataTemplate>
                            </ItemsControl.ItemTemplate>
                        </ItemsControl>
                    </ScrollViewer>
                    <Button Grid.Column="1" x:Name="ToolbarExpander" Width="15" Height="15" Margin="0,2,2,0"
                            VerticalContentAlignment="Center" HorizontalContentAlignment="Center"
                            Style="{StaticResource ExpanderButtonStyle}" Content="&lt;" FontWeight="Bold" Click="ExpanderToolbar_Click">
                    </Button>
                    <sdk:Label Grid.Column="0" x:Name="ToolbarTitle" VerticalContentAlignment="Center"
                               Style="{StaticResource TitlePanelHorizontal}" Content="Toolbar"
                               Margin="3,0,0,0" Background="Transparent">
                    </sdk:Label>
                </Grid>
            </Border>
            <!--</my1:InnerGlowBorder>-->

            <controls:GridSplitter Grid.Column="1" x:Name="spliter1"
                                   VerticalAlignment="Stretch" HorizontalAlignment="Stretch" Style="{StaticResource GridSplitter1Style}">
            </controls:GridSplitter>

            <my1:GlassBorder Grid.Column="2" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" ContentZIndex="1"
                             BorderThickness="0" GlassOpacity="0" Style="{StaticResource DesignCanvasBorderStyle}">
                <Grid HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Style="{StaticResource DesignCanvasGridStyle}">
                    <my4:DesignCanvas x:Name="designCanvas1" IsHitTestVisible="True" Opacity="1"
                                    HorizontalAlignment="Stretch" VerticalAlignment="Stretch" 
                                    SelectPropertiesMenu="designCanvas1_SelectPropertiesMenu" SelectionChanged="designCanvas1_SelectionChanged"
                                    BorderThickness="0" BorderBrush="Transparent">
                    </my4:DesignCanvas>
                </Grid>
            </my1:GlassBorder>

            <controls:GridSplitter Grid.Column="3" x:Name="spliter2"
                                   VerticalAlignment="Stretch" HorizontalAlignment="Stretch" Style="{StaticResource GridSplitter2Style}">
            </controls:GridSplitter>

            <Border Grid.Column="5" x:Name="PropertiesGridPanel" Style="{StaticResource PanelStyle}">
            <!--<my1:InnerGlowBorder BorderThickness="0" CornerRadius="0,0,10,10" 
                                 InnerGlowOpacity="0.5" InnerGlowSize="15"
                                 HorizontalAlignment="Stretch" VerticalAlignment="Stretch" 
                                 x:Name="PropertiesGridPanel"
                                 Grid.Column="5"
                                 Style="{StaticResource PanelStyle}">-->
                <Grid>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="19"></ColumnDefinition>
                        <ColumnDefinition Width="*"></ColumnDefinition>
                    </Grid.ColumnDefinitions>
                    <Grid.RowDefinitions>
                        <RowDefinition Height="19"></RowDefinition>
                        <RowDefinition Height="*"></RowDefinition>
                    </Grid.RowDefinitions>
                    <controls:TabControl Grid.Row="2" Grid.Column="0" Grid.ColumnSpan="2" x:Name="propertiesTabs"
                                         VerticalAlignment="Stretch" HorizontalAlignment="Stretch" TabStripPlacement="Bottom">
                        <controls:TabItem Header="Properties">
                            <propertyGrid:PropertyGrid x:Name="propertiesGrid" 
                                           VerticalAlignment="Stretch" HorizontalAlignment="Stretch" 
                                           GridBorderThickness="0" Foreground="Black">
                            </propertyGrid:PropertyGrid>
                        </controls:TabItem>
                        <controls:TabItem Header="Effect options">
                            <propertyGrid:PropertyGrid x:Name="effectPropertiesGrid" 
                                           VerticalAlignment="Stretch" HorizontalAlignment="Stretch" 
                                           GridBorderThickness="0" Foreground="Black">
                            </propertyGrid:PropertyGrid>
                        </controls:TabItem>
                        <controls:TabItem Header="Event">
                            <my3:EventBindingGrid x:Name="eventBindingGrid"
                                           VerticalAlignment="Stretch" HorizontalAlignment="Stretch">
                            </my3:EventBindingGrid>
                        </controls:TabItem>
                    </controls:TabControl>
                    <Button Grid.Column="0" x:Name="PropertiesGridExpander" Width="15" Height="15" Margin="2,2,0,0"
                            VerticalContentAlignment="Center" HorizontalContentAlignment="Center"
                            Style="{StaticResource ExpanderButtonStyle}" Content="&gt;" Padding="0" FontWeight="Bold" Click="PropertiesGridExpander_Click">
                    </Button>
                    <sdk:Label Grid.Column="1" x:Name="PropertiesGridTitle" Background="Transparent" VerticalContentAlignment="Center"
                               Style="{StaticResource TitlePanelHorizontal}" Content="Options"
                               HorizontalAlignment="Right" HorizontalContentAlignment="Right"
                               Margin="0,0,3,0">
                    </sdk:Label>
                </Grid>
                </Border>
            <!--</my1:InnerGlowBorder>-->
            <sdk:Label x:Name="ToolbarTitleVertical" Content="Toolbar" Grid.RowSpan="1" Grid.ColumnSpan="3" Visibility="Collapsed"
                       VerticalContentAlignment="Stretch" HorizontalContentAlignment="Stretch"
                       Margin="-2,8,0,0" Style="{StaticResource TitlePanelVertical}" 
                       HorizontalAlignment="Left" VerticalAlignment="Top">
                <sdk:Label.RenderTransform>
                    <RotateTransform Angle="90" CenterX="2" CenterY="20"></RotateTransform>
                </sdk:Label.RenderTransform>
            </sdk:Label>
            <sdk:Label x:Name="PropertiesGridTitleVertical" Content="Options" Margin="3,5,-95,0" Visibility="Collapsed"
                       Grid.RowSpan="1" Grid.Column="2" Grid.ColumnSpan="2" Grid.Row="1" 
                       VerticalContentAlignment="Stretch" HorizontalContentAlignment="Stretch" 
                       Style="{StaticResource TitlePanelVertical}" 
                       VerticalAlignment="Top" HorizontalAlignment="Right">
                <sdk:Label.RenderTransform>
                    <RotateTransform Angle="90" CenterX="-26" CenterY="-4"></RotateTransform>
                </sdk:Label.RenderTransform>
            </sdk:Label>
        </Grid>
    </Grid>
</navigation:Page>
